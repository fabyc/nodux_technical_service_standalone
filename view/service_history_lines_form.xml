<form string="History Line" cursor="description">
    <label name="service"/>
    <field name="service" colspan="3"/>
    <notebook colspan="4">
        <page string="General" id="general">
            <label name="date"/>
            <field name="date"/>
            <newline/>
            <label name="description"/>
            <field name="description"/>
            <newline/>
            <label name="password"/>
            <field name="password" widget="password" xexpand="1"/>
            <newline/>
        </page>
    </notebook>
</form>
